
<%if(selectedNotes){ %>
    <div class="row d-flex row-cols-1 ">
      
      <div class="col-8 m-auto gx-1">
        <a href="" class="card notes__card mb-5 mt-5 shadow rounded m-auto ">
          <div class="card-body ">
            <h5 class="card-title"> <b class="h5 font-weight-light text-monospace text-success"> Course : </b>   <%- selectedNotes.course %>  </h5>
            <h5 class="card-title2">  <%- selectedNotes.tutor %>  </h5>
            <p class="card-text"> <%- selectedNotes.content %></p>
            <a  href="http://localhost:8000/studentDashboard" class="btn btn-success rounded-pill btn-block " >BACK </a>
          </div>
        </a>
    
      </div>
    
    
    <% }else {%>
    <h1 class="btn btn-outline-secondary rounded-pill shadow mb-5 mt-5 text-align-center w-100  " >Something went wrong(Check the id )</h1>
    <% } %>
    </div>